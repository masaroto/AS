	<% include partials/headerFunc.ejs %>
	
	<div class="container-fluid">
	    	<table class="table table-light table-striped">
    <thead>
      <tr>
        <th scope="col">Nome</th>
        <th scope="col">Quantidade</th>
        <th scope="col">Pre√ßo</th>
      </tr>
    </thead>
    <tbody>
      <%prods.forEach(function(prod){%>
        
	    <tr>
            <td><%=prod.name%></td>
            <td><%=prod.qtd%></td>
            <td>R$ <%=prod.preco%></td>
            <td> 
                <form class="form-inline" action="/pu/func/prod/<%=prod._id%>?_method=DELETE" method="POST">
                    <a class="btn btn-outline-success my-2 my-sm-0" href="/pu/func/prod/<%=prod._id %>">Edit</a>
                    <button class="btn btn-outline-danger my-2 my-sm-0" type="submit">Delete</button>
                </form>
            </td>
        </tr>
                
            
	    <% }); %>
        
        
    </tbody>
  </table>
	</div>
	
	
	
	
	
	
	
